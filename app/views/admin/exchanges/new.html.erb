<%= form_with url: "/admin/exchanges", scope: :exchange, method: :post do |form| %>
    <%= render "form_fields", f: form %>
    <%= form.submit "Submit Exchange", { :class => "button is-link is-light"} %>
    <%= button_to "Back", admin_exchanges_path, { :method => "get",  :class => "button is-primary is-light"} %>
  <% end %>

  

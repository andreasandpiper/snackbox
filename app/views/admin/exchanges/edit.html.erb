<%= form_for @exchange, url: admin_exchange_path(@exchange), html: { method: :put } do |f| %>
    <%= render "form_fields", f: f %>
    <%= f.submit "Update", { :class => "button is-link is-light"} %>
<% end %>
<%= button_to "Back to exchanges", admin_exchanges_path, { :method => "get",  :class => "button is-primary is-light"} %>
    
      